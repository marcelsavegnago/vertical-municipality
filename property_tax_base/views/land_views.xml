<odoo>
  <data>
  
  <record model="ir.ui.view" id="property_tax_land_form_view">
    <field name="name">property_tax.land.form</field>
    <field name="model">property_tax.land</field>
    <field name="priority" eval="16"/>
    <field name="arch" type="xml">
      <form string="Property Tax Land form">
        <header>
            <field name="status" widget="statusbar"/>
        </header>
        <sheet>
          <group>
            <field name="lot_id"/>
            <field name="owner_id"/>
            <field name="type_id"/>
            <field name="use_id"/>
            <field name="block_id"/>
            <field name="zone_id"/>
            <field name="address"/>
            <field name="number"/>
            <field name="zip"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record model="ir.ui.view" id="property_tax_land_tree_view">
    <field name="name">property_tax.land.tree</field>
    <field name="model">property_tax.land</field>
    <field name="priority" eval="16"/>
    <field name="arch" type="xml">
      <tree string="Property Tax Land tree">
            <field name="lot_id"/>
            <field name="owner_id"/>
            <field name="type_id"/>
            <field name="use_id"/>
            <field name="block_id"/>
            <field name="zone_id"/>
            <field name="address"/>
            <field name="number"/>
            <field name="zip"/>
      </tree>
    </field>
  </record>

  <record model="ir.actions.act_window" id="property_tax_land_action">
    <field name="name">Property Tax Land</field>
    <field name="res_model">property_tax.land</field>
    <field name="view_mode">tree,form</field>
  </record>

  <menuitem name="Property Pymt Mngmt" id="root_menu" sequence="1"/>
    <menuitem name="Property Tax" id="property_tax_menu" parent="root_menu" sequence="10"/>
      <menuitem name="Settings" id="property_tax_settings_menu" parent="property_tax_menu" sequence="10"/>
        <menuitem name="Property Land" id="property_land_menu" parent="property_tax_settings_menu" action="property_tax_land_action" sequence="10"/>
  
  </data>
</odoo>

